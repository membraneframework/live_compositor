{
    "compilerOptions": {
        "module": "commonjs",
        "strict": true,
        "jsx": "react-jsx",
        "target": "es2015",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "declaration": true
    }
}
